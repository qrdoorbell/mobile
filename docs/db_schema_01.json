        "titleText": { "type": "text", "limit": 80, "value_en": "Please scan QR code to ring the doorbell", "value_ua": "Проскануйте QR код щоб подзвонити в дверний дзвоник" }
      }
    },
    "stick-vrtcl-tpl": {
      "handler": "[anotherHandlerClassName]",
      "config": {
        "imageUrl": { "type": "url", "hidden": true, "value": "" },
        "iconUrl": { "type": "icon-select", "options": [ "ac_unit_outlined", "abc_outlined" ] },
        "titleText": { "type": "text", "limit": 80, "value_en": "Please scan QR code to ring the doorbell", "value_ua": "Проскануйте QR код щоб подзвонити в дверний дзвоник" }
